<div class="col-xs-8 col-xs-offset-2">
  <h2>Carrier Verification</h2>

    <div class='col-xs-6 title'>
      Name
    </div>
    <div class='col-xs-6 title'>
      MC Number
    </div>

    <% @carriers.each do |c| %>
      <div class='col-xs-6'>
        <%= c.name %>
      </div>
      <div class='col-xs-6'>
        <%= c.MCNumber %>
      </div>
    <% end %>
  
</div>